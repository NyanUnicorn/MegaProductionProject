<UserControl x:Class="MegaProduction.View.Fragments.UCs.UCWebsiteManager"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MegaProduction.View.Fragments.UCs"
             mc:Ignorable="d" 
             d:DesignHeight="451.6" d:DesignWidth="800">
    <Grid Background="#FF6C6D6B">
        <TabControl x:Name="TabCtrlWebview" Margin="0,0,0,0.2" Background="#FF6C6D6B">
            <TabItem Header="TabItem">
                <Grid Background="#FF6C6D6B"/>
            </TabItem>
            <TabItem Header="IndexTest">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</UserControl>
